{
  "name": "@adefee/palworld-save-editor",
  "version": "1.0.0",
  "description": "Comprehensive and extensible save editor and reporting tool for Palworld",
  "main": "index.js",
  "bin": "./index.js",
  "scripts": {
    "install-packager": "npm install -g pkg",
    "package": "pkg . --targets node18-win-x64 --output ./bin/paver-win --compress Brotli",
    "package-all": "pkg . --targets node18-linux-x64,node18-macos-x64,node18-win-x64 --output ./bin/paver --compress Brotli",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-3.0",
  "dependencies": {
    "dotenv": "^16.4.1",
    "minimist": "^1.2.8",
    "readline": "^1.3.0"
  }
}
